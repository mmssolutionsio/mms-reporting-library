{
  "name": "multivisio-ld",
  "label": "skeleton",
  "version": "1.0.51",
  "author": "nsgroup",
  "assets": {
    "css": [
      "./assets/ldd.css"
    ],
    "js": []
  },
  "wrapper": "<div class=\"srl-article-root doc-section\"></div>",
  "imageRatios": {
    "cover": {
      "label": "Cover",
      "ratio": "210x205"
    },
    "a4": {
      "label": "A4",
      "ratio": "70x99"
    },
    "3:2": {
      "label": "3:2",
      "ratio": "3x2"
    },
    "1:1": {
      "label": "1:1",
      "ratio": "1x1"
    },
    "4:3": {
      "label": "4:3",
      "ratio": "4x3"
    },
    "16:9": {
      "label": "16:9",
      "ratio": "16x9"
    }
  },
  "componentProperties": {
    "round-portrait": {
      "label": "Round portrait",
